# Vi Overseer Configuration

# Port for the Overseer daemon (God Console connects here)
OVERSEER_PORT=3200

# Initial TEST_MODE state
VI_TEST_MODE=false

# Logging
LOG_LEVEL=info
NODE_ENV=development

# Paths (relative to where Overseer runs)
# These must point to the service directories
VI_CORE_PATH=../../core/vi
SOVEREIGN_PATH=../../clients/command/sovereign
